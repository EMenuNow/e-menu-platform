
<div class="row">
    <div class="col"> 
      <ul>
      <% @restaurants.each do |restaurant| %>
        <li><%= link_to(image_tag(restaurant.logo.variant(resize_to_limit: [100, 100])), restaurant_url_path(restaurant.url) ) if restaurant.logo.attached? %></li>
      <% end %>
      <ul>
    </div>
</div>