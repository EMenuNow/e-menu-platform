<% content_for :title do %>
Partner with EMenu Now
<% end %>

<div class="row">
  <div class="col">&nbsp;</div>
    <% if current_onboarding_restaurant_user %>
      <div class="col-lg-6">
        <div class="card p-3 mb-5 bg-white rounded">
          <div class="card-body text-center">
            <%= link_to("Continue", onboarding_continue_path, class: "nav-link") %>
          </div>
        </div>
      </div>
    <% else %>
      <div class="col-lg-6">
        <div class="card p-3 mb-5 bg-white rounded">
          <div class="card-body text-center">
            <%= link_to("Sign Up", new_onboarding_restaurant_user_registration_path, class: "nav-link") %>
          </div>
        </div>
        <div class="card p-3 mb-5 bg-white rounded">
          <div class="card-body text-center">
            <%= link_to("Sign In", new_onboarding_restaurant_user_session_path, class: "nav-link") %>
          </div>
        </div>
      </div>
    <% end %>
  <div class="col">&nbsp;</div>
</div>